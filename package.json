{
  "name": "nsvueapp",
  "version": "1.0.0",
  "description": "NativeScript-Vue application",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && cross-env NODE_ENV=production node index.js",
    "build:android": "cross-env PLATFORM=android npm run build",
    "build:ios": "cross-env PLATFORM=ios npm run build",
    "clean": "rimraf dist",
    "watch": "node index.js",
    "watch:android": "cross-env PLATFORM=android npm run watch",
    "watch:ios": "cross-env PLATFORM=ios npm run watch"
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.1.9",
    "apollo-client": "^2.2.5",
    "apollo-link": "^1.2.1",
    "apollo-link-http": "^1.5.1",
    "graphql": "^0.13.1",
    "graphql-tag": "^2.8.0",
    "nativescript-vue": "^1.2.0",
    "tns-core-modules": "3.4.1",
    "vue-apollo": "^3.0.0-beta.4",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "cross-env": "^5.1.3",
    "cross-spawn": "^6.0.4",
    "css-loader": "^0.28.10",
    "extract-text-webpack-plugin": "^3.0.2",
    "fs-extra": "^5.0.0",
    "nativescript-vue-loader": "^0.1.5",
    "nativescript-vue-target": "^0.1.0",
    "nativescript-vue-template-compiler": "^1.2.0",
    "node-sass": "^4.7.2",
    "ns-vue-loader": "^0.1.2",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "vue-template-compiler": "^2.5.13",
    "webpack": "^3.11.0",
    "winston-color": "^1.0.0"
  },
